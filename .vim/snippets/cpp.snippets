# for unsigned int
snippet forui
	for (unsigned int ${1:i} = ${2:0}; $1 < ${3:count}; $1++) {
		${4}
	}

# for int
snippet fori
	for (int ${1:i} = ${2:0}; $1 < ${3:count}; $1++) {
		${4}
	}

# header
snippet pragma
	#ifndef ${1:HEADER}
	#define $1

	${2}

	#endif

# std::cout
snippet cout
	std::cout << ${1}'\n';${2}

# std::cout
snippet coute
	std::cout << ${1}std::endl;${2}
